{
    "manifest_version": 2,
    "name": "Speed Dial",
    "version": "0.1.3",

    "description": "The end is nigh!",
    "icons": {
        "16": "icons/tch_16.png",
        "48": "icons/tch_48.png",
        "128": "icons/tch_128.png"
    },

    "permissions": [
      "storage"
    ],

    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://code.jquery.com; object-src 'self'"
}
